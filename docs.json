  "navbar": {
    "links": [
      {
        "type": "github",
        "href": "https://github.com/rovony/ZajalyDocs_Mintlify"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.zajaly.dev"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor",
      "mcp"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/rovony",
      "x": "https://x.com/zajaly"
    },
    "links": [
      {
        "header": "Resources",
        "items": [
          { "label": "Blog", "href": "https://blog.zajaly.dev" },
          { "label": "Changelog", "href": "/changelog" }
        ]
      },
      {
        "header": "Products",
        "items": [
          { "label": "Custojo", "href": "/products/custojo/overview" },
          { "label": "Waraq", "href": "/products/waraq/overview" },
          { "label": "DeployVault", "href": "/products/deployvault/overview" }
        ]
      }
    ]
