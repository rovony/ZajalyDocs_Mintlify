    "default": "dark"
  },
  "fonts": {
    "heading": {
      "family": "Montserrat",
      "weight": 400
    },
    "body": {
      "family": "Poppins",
      "weight": 400
    }
  },
  "styling": {
    "eyebrows": "section",
    "latex": true
  },
  "icons": {
    "library": "fontawesome"
  },
  "search": {
    "prompt": "Search docs..."
  },
  "seo": {
    "metatags": {
      "og:site_name": "Zajaly Developer Docs"
    },
    "indexing": "navigable"
  }
